    "@dillonkearns/elm-graphql": "^4.0.5",
    "@types/node": "^10.9.4",
    "@types/serviceworker-webpack-plugin": "^1.0.1",
    "@types/webpack": "^5",
    "autoprefixer": "^10.2.1",
    "chokidar": "^3.5.1",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.2",
    "elm": "^0.19.1-3",
    "elm-hot-webpack-loader": "^1.1.8",
    "elm-live": "^4.0.2",
    "elm-ts-interop": "^0.0.8",
    "elm-webpack-loader": "^7.0.1",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-elm-tailwind": "^0.10.0",
    "postcss-loader": "^5.0.0",
    "purgecss": "^3.1.3",
    "serviceworker-webpack-plugin": "^1.0.1",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "ts-loader": "^5.1.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3",
    "webpack": "^5",
    "webpack-cli": "^4",
    "webpack-dev-server": "^4.9.2",
    "workbox-webpack-plugin": "^6.1.0"
  },
  "name": "monis-app",
  "version": "1.0.0",
  "description": "Monis App",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "yarn gen:tailwind && yarn gen:typescript && webpack -p",
    "dev": "concurrently -k -n w: npm:watch:*",
    "watch:tailwind": "chokidar tailwind.config.js main.css postcss.config.js --initial -c 'yarn gen:tailwind'",
    "watch:webpack": "chokidar ./src/Ports.elm --initial -c 'yarn serve'",
    "\"watch:webpack\": \"chokidar ./src/Ports.elm --initial -c 'yarn gen:typescript && yarn serve'\"" : "",
    "gen:graphql": "elm-graphql http://127.0.0.1:4000/graphiql --base MonisApp --output src",
    "gen:tailwind": "postcss -o dist/main.css main.css",
    "gen:typescript": "elm-ts-interop",
    "serve": "webpack serve"
  }
}

{
  "devDependencies": {
    "@dillonkearns/elm-graphql": "^4.0.5",
    "autoprefixer": "^10.2.1",
    "elm": "^0.19.1-3",
    "elm-live": "^4.0.2",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-elm-tailwind": "^0.10.0",
    "purgecss": "^3.1.3",
    "tailwindcss": "^2.0.2"
  },
  "name": "monis-app",
  "version": "1.0.0",
  "description": "Monis App",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn tailwind:gen && yarn live",
    "live": "elm-live src/Main.elm -H -d=./dist --start-page=index.html --pushstate -- --output=dist/app.js",
    "graphql:gen": "elm-graphql http://127.0.0.1:4000/graphiql --base MonisApp --output src",
    "tailwind:gen": "postcss -o dist/main.css main.css"
  }
}
